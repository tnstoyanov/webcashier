{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run WebCashier",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run",
				"--launch-profile",
				"https"
			],
			"group": "build",
			"isBackground": true,
			"problemMatcher": [
				"$msCompile"
			],
			"options": {
				"cwd": "${workspaceFolder}/WebCashier"
			}
		}
	]
}